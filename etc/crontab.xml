<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="run_cart_add_consumer" instance="Sizebay\SizebayTracker\Cron\RunCartAddConsumer" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="s" instance="Sizebay\SizebayTracker\Cron\RunOrderConsumer" method="execute">
            <schedule>* * * * *</schedule> 
        </job>
    </group>
</config>
